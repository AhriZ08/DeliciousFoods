@font-face {font-family: "iconfont";
  src: url(data:application/vnd.ms-fontobject;base64,); /* IE9 */
  src: url(data:application/vnd.ms-fontobject;base64,#iefix) format('embedded-opentype'), 
  url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2'),
  url(data:font/woff;base64,) format('woff'),
  url(data:font/ttf;base64,) format('truetype'), 
  url(../../static/img/iconfont.24a4429c.svg#iconfont) format('svg'); /* iOS 4.1- */
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.icon-caiwu:before {
  content: "\e604";
}
.icon-pingjia:before {
  content: "\e601";
}
.icon-shangpin:before {
  content: "\e6c9";
}
.icon-icon--copy:before {
  content: "\e600";
}
.content {
	background-color:#d8d8d8;
}
.content text{
	font-size: large;
}


/*第一行 店铺信息 */
.Information_shop{
	width: 100%;
	height: 400rpx;
	border-color:#999999;
	border-style:double;
	border-size:10rpx;
	background-color:#0091EB;
}
.Information_shop image{
	border: 10rpx;
	border-style: inset;
	margin-left: 30rpx;
	width: 200rpx;
	height: 200rpx;
	position: absolute;
	margin-top: 100rpx;
}
.Information_shop text{
	position: absolute;
}
.Information_shop text:nth-child(2){
	margin-top: 120rpx;
	margin-left: 300rpx;
	color:#F8F8F8;
}
.Information_shop text:nth-child(3){
	margin-top: 260rpx;
	margin-left: 300rpx;
	font-weight: 50;
}
.Information_shop text:nth-child(4){
	margin-top: 260rpx;
	margin-left: 600rpx;
	background-color: #007AC7;
	border-radius: 20rpx;
	color:#F8F8F8;
}


/*第二行 店铺操作 */
.opration{
	background-color: #F8F8F8;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	margin-top: 40rpx;
	border-color:#FFFFFF;
	border-style:double;
	border-size:10rpx;
}
.opration_item{
	width: 25%;
	text-align: center;
}
.opration_item view{
	font-size: 100rpx;
	text-align: center;
	margin: 0rpx auto;
	border-style: none;
}
.opration_item text{
	margin-top: 0rpx;
}


/*第三行 经营数据 */
.Turnover{
	margin-top: 40rpx;
	background-color: #F8F8F8;
	border-color: #FFFFFF;
	border-style:double;
	border-size:10rpx;
}
.Turnover_Head{
	
	border-bottom-color: #999999;
	border-bottom-width: 5rpx;
	border-bottom-style: groove;
}
.Turnover_Head text:nth-child(2){
	font-weight: 50;
	font-size:x-large;
	float: right;
}
.Turnover_Head text:nth-child(1){
	font-weight: 500;
	font-size:x-large;
}
.Turnover_content{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	width: 100%;
	padding-top: 20rpx;
}
.Turnover_content view:nth-child(1){
	border-right-color: #b2d9a1;
	border-right-width: 
	5rpx;border-right-style: groove;
}
.Turnover_content view:nth-child(2){
	border-right-color: #b2d9a1;
	border-right-width: 5rpx;
	border-right-style: groove;
}
.Turnover_content_div
{
	width: 33%;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	text-align: center;
}
.Turnover_content_div text:nth-child(2)
{
	font-weight: 600;
}
.Turnover_content_div text:nth-child(3)
{
	font-weight: 50;
}

